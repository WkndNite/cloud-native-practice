            "vmArgs": "-Dserver.port=8010"
        },
        {
            "type": "java",
            "name": "CRUDServiceApplication - Port 8011",
            "request": "launch",
            "mainClass": "com.example.crud.CRUDServiceApplication",
            "projectName": "crud-service",
            "vmArgs": "-Dserver.port=8011"
        },
        {
            "type": "java",
            "name": "Gateway",
            "request": "launch",
            "mainClass": "com.example.gateway.GatewayServiceApplication",
            "projectName": "gateway",
